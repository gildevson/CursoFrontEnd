{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@pages/*": ["src/components/pages/*"],
      "@components/*": ["src/components/*"],
      "@services/*": ["src/services/*"],
      "@routes/*": ["src/routes/*"]
    },
    "types": ["vite/client"]
  },
  "include": ["src"]
}
